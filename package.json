{
  "name": "angular2-seed",
  "version": "0.0.0",
  "engines": {
      "node" : "4.2.1",
      "npm" : "3.3.8"
  },
  "description": "Seed for Angular 2 apps",
  "repository": "https://github.com/mgechev/angular2-seed",
  "scripts": {
    "build.dev": "gulp build.dev",
    "build.dev.watch": "gulp build.dev.watch",
    "build.prod": "gulp build.prod --env prod",
    "build.test": "gulp build.test",
    "build.test.watch": "gulp build.test.watch",
    "gulp": "gulp",
    "karma": "karma",
    "karma.start": "karma start",
    "lint": "gulp tslint",
    "postinstall": "tsd reinstall --clean && tsd link && tsd rebundle && gulp check.versions && gulp postinstall && gulp build.prod",
    "reinstall": "npm cache clean && rm -rf node_modules && npm install",
    "start": "node dist/server/app.js --env prod",
    "serve.dev": "gulp serve --env dev",
    "tasks.list": "gulp --tasks-simple",
    "test": "gulp test",
    "tsd": "tsd",
    "docs": "npm run gulp -- build.docs && npm run gulp -- serve.docs"
  },
  "author": "Minko Gechev <mgechev>",
  "license": "MIT",
